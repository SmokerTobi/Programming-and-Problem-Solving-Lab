s1=float(input("subject 1: "))
s2=float(input("subject 2: "))
s3=float(input("subject 3: "))
s4=float(input("subject 4: "))
s5=float(input("subject 5: "))
a=(s1+s2+s3+s4+s5)/5
print("Average Marks: %.2f"%(a))
if a>=90 :
	print("Grade: A",)
elif a>=80 and a<=89:
	print("Grade: B",)
elif a>=70 and a<=79:
	print("Grade: C",)
elif a>=60 and a<=69:
	print("Grade: D")
elif a<60 and a>0:
	print("Grade: F")
